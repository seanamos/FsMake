<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <OtherFlags>$(OtherFlags)--warnon:3390</OtherFlags>
  </PropertyGroup>
  <PropertyGroup>
    <PackageId>FsMake</PackageId>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright (c) 2021 Sean Amos</Copyright>
    <Authors>seanamos</Authors>
    <Description>F# pipeline step/task runner</Description>
    <PackageTags>fsmake make build fsi f# fsharp pipeline step</PackageTags>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/seanamos/FsMake</RepositoryUrl>
    <PackageProjectUrl>https://seanamos.github.io/FsMake/</PackageProjectUrl>
    <PackageReleaseNotes>https://github.com/seanamos/FsMake/releases</PackageReleaseNotes>
    <PackageIcon>nuget-logo.png</PackageIcon>
  </PropertyGroup>
  <PropertyGroup>
    <FsDocsLogoLink>https://seanamos.github.io/FsMake</FsDocsLogoLink>
    <FsDocsReleaseNotesLink>https://github.com/seanamos/FsMake/releases</FsDocsReleaseNotesLink>
  </PropertyGroup>
  <ItemGroup>
    <None Include="../README.md" Pack="true" PackagePath="" />
    <None Include="../assets/nuget-logo.png" Pack="true" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="Properties/AssemblyInfo.fs" />
    <Compile Include="Console.fs" />
    <Compile Include="Glob.fs" />
    <Compile Include="Extensions.fs" />
    <Compile Include="Prefix.fs" />
    <Compile Include="Cli.fs" />
    <Compile Include="ProcessMonitor.fs" />
    <Compile Include="Make.fs" />
    <Compile Include="EnvVar.fs" />
    <Compile Include="Step.fs" />
    <Compile Include="Cmd.fs" />
    <Compile Include="ParallelMaybe.fs" />
    <Compile Include="Stage.fs" />
    <Compile Include="Pipeline.fs" />
    <Compile Include="Pipelines.fs" />
  </ItemGroup>
  <Import Project="..\.paket\Paket.Restore.targets" />
</Project>
